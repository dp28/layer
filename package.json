{
  "name": "layer",
  "version": "0.0.0",
  "description": "A command-line tool to convert HTML pages to backgrounds for the gnome terminal.",
  "author": "Daniel Patterson",
  "license": "MIT",
  "preferGlobal": true,
  "bin": {
    "layer": "bin/layer.coffee",
    "layer-append": "bin/layer-append.coffee",
    "layer-config": "bin/layer-config.coffee",
    "layer-delete": "bin/layer-delete.coffee",
    "layer-list": "bin/layer-list.coffee",
    "layer-new": "bin/layer-new.coffee",
    "layer-read": "bin/layer-read.coffee",
    "layer-render": "bin/layer-render.coffee",
    "layer-show": "bin/layer-show.coffee",
    "layer-write": "bin/layer-write.coffee"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dp28/layer.git"
  },
  "keywords": [
    "terminal",
    "background"
  ],
  "bugs": {
    "url": "https://github.com/dp28/layer/issues"
  },
  "dependencies": {
    "bluebird": "~2.10.2",
    "chalk": "^1.1.1",
    "coffee-script": "~1.10.0",
    "jade": "~1.11.0",
    "path": "~0.12.7",
    "phantom": "~0.8.0",
    "spawn-sync": "~1.0.13",
    "yargs": "~3.27.0"
  }
}
